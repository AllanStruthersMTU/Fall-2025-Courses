Cell[TextData[{
 "A scalar ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["1", "st"], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "636da837-f79a-420a-9836-2ace031af3e9"],
 " order linear ODE has the standard form\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"y", "'"}], "+", 
     RowBox[{
      RowBox[{"p", "(", "t", ")"}], "y"}]}], "=", 
    RowBox[{"q", "(", "t", ")"}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "85cf0f2f-ba3c-4c45-950a-192817621c5c"],
 "\nSince the integrating factor\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Mu]", "(", "t", ")"}], "=", 
    RowBox[{
     SuperscriptBox["\[ExponentialE]", 
      RowBox[{
       SuperscriptBox["\[Integral]", "t"], 
       RowBox[{
        RowBox[{"p", "(", "\[Tau]", ")"}], "d\[Tau]"}]}]], ">", "0"}]}], 
   TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "cb82e908-91db-44e4-83ff-fa949dfacb0a"],
 "\nhas derivative ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"\[Mu]", "'"}], 
     RowBox[{"(", "t", ")"}]}], "=", 
    RowBox[{
     RowBox[{"\[Mu]", "(", "t", ")"}], " ", 
     RowBox[{"p", "(", "t", ")"}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "aef8e620-6be6-4b14-9d0f-2d50dc59fada"],
 " we get\n\t",
 Cell[BoxData[
  FormBox[GridBox[{
     {" ", 
      RowBox[{"y", "'"}], "+", 
      RowBox[{"p", " ", "y"}], "=", "q"},
     {"\[DoubleLongLeftRightArrow]", 
      RowBox[{"\[Mu]", " ", 
       RowBox[{"y", "'"}]}], "+", 
      RowBox[{"\[Mu]", " ", "p", " ", "y"}], "=", 
      RowBox[{"\[Mu]", " ", "q"}]},
     {"\[DoubleLongLeftRightArrow]", 
      RowBox[{"\[Mu]", " ", 
       RowBox[{"y", "'"}]}], "+", 
      RowBox[{
       RowBox[{"\[Mu]", "'"}], " ", "y"}], "=", 
      RowBox[{"\[Mu]", " ", "q"}]},
     {"\[DoubleLongLeftRightArrow]", " ", " ", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"\[Mu]", " ", "y"}], ")"}], "'"}], "=", 
      RowBox[{"\[Mu]", " ", "q"}]}
    }], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "9baa9f21-ee7b-4ad4-b568-8aa715bf9b60"],
 "\nIntegrating both sides gives\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{"\[Mu]", "(", "t", ")"}], " ", 
      RowBox[{"y", "(", "t", ")"}]}], "-", 
     RowBox[{
      RowBox[{"\[Mu]", "(", "0", ")"}], 
      RowBox[{"y", "(", "0", ")"}]}]}], "=", 
    RowBox[{
     SubsuperscriptBox["\[Integral]", "0", "t"], 
     RowBox[{
      RowBox[{"\[Mu]", "(", "\[Tau]", ")"}], 
      RowBox[{"q", "(", "\[Tau]", ")"}], " ", "d\[Tau]"}]}]}], 
   TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "cc9f9f61-d11b-49d0-baff-65210986b335"],
 "."
}], "Text",
