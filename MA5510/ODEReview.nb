Cell[TextData[{
 "Ex 1. The ODE  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"y", "''"}], "=", 
    RowBox[{
     RowBox[{"A", ".", "y"}], "+", "b"}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "0a72f694-fcef-4617-bdd2-06a033858f48"],
 " with ",
 Cell[BoxData[
  FormBox[
   RowBox[{"A", "=", 
    RowBox[{"(", GridBox[{
       {"5", "4"},
       {"4", "5"}
      }], ")"}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "8a897e85-2c6f-4212-a953-54731f79d98d"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{"b", "=", 
    RowBox[{"(", GridBox[{
       {"0"},
       {"9"}
      }], ")"}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "a3f8b05d-71df-4964-a6b9-c5073d14d472"],
 " is a second order linear ODE for a function ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"y", ":", "\[DoubleStruckCapitalR]"}], "\[Rule]", 
    SuperscriptBox["\[DoubleStruckCapitalR]", "2"]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "1bf765a5-60a7-463c-a969-aef7ea7997e6"],
 ".  One solution of the ODE is ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"y", "(", "t", ")"}], "=", 
    RowBox[{"(", GridBox[{
       {"4"},
       {
        RowBox[{"-", "5"}]}
      }], ")"}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "4f1d0652-d959-4069-a2a4-4ca4f209383d"],
 ".  \nAnother solution is ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"y", "(", "t", ")"}], "=", 
    RowBox[{
     FractionBox["1", "4"], " ", 
     RowBox[{"(", GridBox[{
        {
         RowBox[{"16", "+", 
          SuperscriptBox["\[ExponentialE]", 
           RowBox[{
            RowBox[{"-", "3"}], " ", "t"}]], "+", 
          SuperscriptBox["\[ExponentialE]", 
           RowBox[{"-", "t"}]], "+", 
          SuperscriptBox["\[ExponentialE]", "t"], "+", 
          SuperscriptBox["\[ExponentialE]", 
           RowBox[{"3", " ", "t"}]]}]},
        {
         RowBox[{
          RowBox[{"-", "20"}], "+", 
          SuperscriptBox["\[ExponentialE]", 
           RowBox[{
            RowBox[{"-", "3"}], " ", "t"}]], "-", 
          SuperscriptBox["\[ExponentialE]", 
           RowBox[{"-", "t"}]], "-", 
          SuperscriptBox["\[ExponentialE]", "t"], "+", 
          SuperscriptBox["\[ExponentialE]", 
           RowBox[{"3", " ", "t"}]]}]}
       }], ")"}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "0792def1-d281-43a3-acb3-a87b30ad2861"],
 ". \nYet another solution is ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"y", "(", "t", ")"}], "=", 
    RowBox[{
     FractionBox["1", "12"], " ", 
     RowBox[{"(", GridBox[{
        {
         RowBox[{"48", "-", 
          SuperscriptBox["\[ExponentialE]", 
           RowBox[{
            RowBox[{"-", "3"}], " ", "t"}]], "-", 
          RowBox[{"3", " ", 
           SuperscriptBox["\[ExponentialE]", 
            RowBox[{"-", "t"}]]}], "+", 
          RowBox[{"3", " ", 
           SuperscriptBox["\[ExponentialE]", "t"]}], "+", 
          SuperscriptBox["\[ExponentialE]", 
           RowBox[{"3", " ", "t"}]]}]},
        {
         RowBox[{
          RowBox[{"-", "60"}], "-", 
          SuperscriptBox["\[ExponentialE]", 
           RowBox[{
            RowBox[{"-", "3"}], " ", "t"}]], "+", 
          RowBox[{"3", " ", 
           SuperscriptBox["\[ExponentialE]", 
            RowBox[{"-", "t"}]]}], "-", 
          RowBox[{"3", " ", 
           SuperscriptBox["\[ExponentialE]", "t"]}], "+", 
          SuperscriptBox["\[ExponentialE]", 
           RowBox[{"3", " ", "t"}]]}]}
       }], ")"}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "6cb66033-bfa6-46cd-8c9a-0c3a795109e0"],
 ". "
}], "Text",
 CellChangeTimes->{{3.9582128808065023`*^9, 3.9582128917451754`*^9}, {
