      RowBox[{
       RowBox[{"(", 
        RowBox[{
         SubscriptBox["t", "0"], ",", 
         SubscriptBox["y", "0"], ",", 
         SubscriptBox["v", "0"]}], ")"}], ".", " "}]}]}]], TraditionalForm]],
