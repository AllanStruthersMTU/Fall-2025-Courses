Cell[TextData[{
 "Standard Existence and Uniqueness Theorems\n",
 Cell[BoxData[
  FormBox[
   FrameBox[
    RowBox[{
     RowBox[{"The", " ", 
      SuperscriptBox["1", "st"], " ", "order", " ", "IVP"}], " ", "\n", "\t", 
     RowBox[{
      RowBox[{
       RowBox[{"y", "'"}], " ", 
       RowBox[{"(", "t", ")"}]}], " ", "=", " ", 
      RowBox[{
       RowBox[{
        RowBox[{"f", "(", 
         RowBox[{"t", ",", 
          RowBox[{"y", "(", "t", ")"}], ","}], ")"}], "  ", "and", " ", 
        RowBox[{"y", "(", 
         SubscriptBox["t", "0"], ")"}]}], " ", "=", 
       SubscriptBox["y", "0"]}]}], " ", "\n", 
     RowBox[{
     "has", " ", "a", " ", "unique", " ", "solution", " ", "near", " ", 
      SubscriptBox["t", "0"], " ", "provided"}], " ", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"f", " ", ":", " ", 
       RowBox[{"(", 
        RowBox[{"\[DoubleStruckCapitalR]", "\[Times]", 
         SuperscriptBox["\[DoubleStruckCapitalR]", "n"]}], ")"}]}], "\[Rule]", 
      SuperscriptBox["\[DoubleStruckCapitalR]", "n"]}], "\n", 
     RowBox[{
     "is", " ", "nice", " ", "near", " ", "the", " ", "initial", " ", "point",
       " ", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         SubscriptBox["t", "0"], ",", 
         SubscriptBox["y", "0"]}], ")"}], ".", " "}]}]}]], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "8281c292-19a1-4c7f-b595-8564e5ca3c1d"],
 "\n",
 Cell[BoxData[
  FormBox[
   FrameBox[
    RowBox[{
     RowBox[{"The", " ", 
      SuperscriptBox["2", "nd"], "order", " ", "IVP"}], " ", "\n", "\t", 
     RowBox[{
      RowBox[{
       RowBox[{"y", "''"}], " ", 
       RowBox[{"(", "t", ")"}]}], " ", "=", " ", 
      RowBox[{
       RowBox[{
        RowBox[{"f", "(", 
         RowBox[{"t", ",", 
          RowBox[{"y", "(", "t", ")"}], ",", 
          RowBox[{
           RowBox[{"y", "'"}], 
           RowBox[{"(", "t", ")"}]}]}], ")"}], "  ", "and", " ", 
        RowBox[{"y", "(", 
